generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model Todo {
  id        Int      @id @default(autoincrement())
  title     String   @db.VarChar(255)
  isDone    Boolean  @default(false)
  createdAt DateTime @default(now())
  updateAt  DateTime @updatedAt
}
